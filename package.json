  "dependencies": {
    "docpad": "6.59.x",
    "docpad-plugin-eco": "2.0.2",
    "docpad-plugin-marked": "2.1.1",
    "docpad-plugin-partials": "2.6.1",
    "docpad-plugin-less": "2.1.3",
    "docpad-plugin-ghpages": "2.0.2"
  }
