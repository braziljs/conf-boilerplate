{
  "name": "Front in Fortaleza",
  "version": "0.2.0",
  "description": "O melhor Frontin do Nordeste, com os melhores do Brasil.",
  "homepage": "http://frontinfortaleza.com.br",
  "repository": {
    "type" : "git",
    "url" : "http://github.com/faeliaso/front-in-fortaleza.git"
  },
  "engines": {
    "node": ">0.10.x",
    "npm": "1.3.x"
  },
